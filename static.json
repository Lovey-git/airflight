{
    "routes": {
      "/**": "index.html"
    },
    "headers": {
      "ngsw-worker.js": {
        "Cache-Control": "no-cache",
        "Content-Security-Policy": "default-src 'self';img-src 'self' https://air-food.herokuapp.com;"
      },
      "/build/app/**": {
        "Cache-Control": "public, max-age=31536000"
      }
    },
    "root": "www/"
  }